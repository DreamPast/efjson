{
  "name": "efjson",
  "version": "0.7.0",
  "description": "a streaming and event-driven JSON parser (with support for JSON5 and JSONC)",
  "main": "dist/efjson.js",
  "types": "dist/efjson.d.ts",
  "keywords": [
    "json",
    "streaming",
    "event-driven",
    "json5",
    "jsonc"
  ],
  "author": "Jin Cai",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "gen_cjs": "uglifyjs dist/efjson.js --config-file uglifyjs.json --output dist/efjson.min.cjs --wrap efjson",
    "gen_mjs": "uglifyjs dist/efjson.js --config-file uglifyjs.json --output dist/efjson.min.mjs --module",
    "build_cjs": "tsc && npm run gen_cjs",
    "build_mjs": "tsc --module es6 && npm run gen_mjs",
    "compress": "uglifyjs -c -m --output efjson.min.js efjson.js",
    "prepare": "npm run build_mjs && npm run build_cjs"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "uglify-js": "^3.19.3"
  },
  "homepage": "https://github.com/DreamPast/efjson#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreamPast/efjson.git"
  }
}